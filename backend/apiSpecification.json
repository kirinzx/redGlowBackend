            "name": "username",
            "description": "Имя пользователя",
            "schema": {
              "type": "string",
              "example": "kirin"
            }
          },
          {
            "in": "path",
            "required": true,
            "name": "page",
            "description": "Страница, начиная с 1",
            "schema": {
              "type": "integer",
              "example": 1
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Успешно получен0 20 друзей",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Friends"
                }
              }
            }
          },
          "403": {
            "description": "Запрос от неавторизованного пользователя"
          },
          "404": {
            "description": "Пользователь не найден"
          },
          "default": {
            "description": "Все остальное"
          }
        }
      }
    },
