            "required": true,
            "schema": {
              "type": "string",
              "example": "saf141kncaskn214"
            }
          },
          {
            "in": "path",
            "name": "username",
            "description": "Имя пользователя",
            "required": true,
            "schema": {
              "type": "string",
              "example": "kirin"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Ответ со статусом дружбы",
